{
  "version": 2,
  "name": "sebitam-portal",
  "cleanUrls": true,
  "framework": null,
  "installCommand": "echo 'Static Site'",
  "buildCommand": "echo 'Ready'",
  "outputDirectory": ".",
  "headers": [
    {
      "source": "/(.*).html",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=UTF-8"
        }
      ]
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html; charset=UTF-8"
        }
      ]
    }
  ]
}